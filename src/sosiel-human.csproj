<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>Landis.Extension.SOSIELHuman</RootNamespace>
    <AssemblyName>Landis.Extension.SOSIELHuman-v2</AssemblyName>
    <AssemblyTitle>Landis.Extension.SOSIELHuman-v2</AssemblyTitle>
    <AssemblyCompany>Landis-II-Foundation</AssemblyCompany>
